{
    "name": "streamr-client",
    "version": "6.0.1",
    "description": "JavaScript client library for Streamr",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/streamr-dev/network-monorepo.git",
        "directory": "packages/client"
    },
    "types": "./dist/types/src/index.d.ts",
    "main": "./dist/src/index-commonjs.js",
    "browser": "./dist/streamr-client.web.js",
    "exports": {
        "browser": "./dist/streamr-client.web.js",
        "default": {
            "import": "./dist/src/index-esm.mjs",
            "require": "./dist/src/index-commonjs.js"
        }
    },
    "scripts": {
        "build": "npm run bootstrap-dist && npm run build-node",
        "build-production": "npm run clean; npm run bootstrap-dist && NODE_ENV=production npm run build-node && npm run build-browser-production",
        "check": "tsc -p ./tsconfig.jest.json --noEmit",
        "vendor": "SRCDIR=`node -p \"path.dirname(require.resolve('quick-lru'))\"` bash -c 'mkdir -p vendor/quick-lru; cp -n $SRCDIR/index.d.ts vendor/quick-lru; cp -n $SRCDIR/index.js vendor/quick-lru'; true",
        "copy-package": "mkdir -p dist/; node copy-package.js; cp -f README.md LICENSE readme-header-img.png dist; mkdir -p dist/src/encryption/migrations; cp -f ./src/encryption/migrations/* dist/src/encryption/migrations; true",
        "bootstrap-dist": "npm run vendor && npm run copy-package && npm run fix-esm",
        "fix-esm": "mkdir -p dist/src/; cp -Rf src/index-esm.mjs dist/src/index-esm.mjs",
        "build-node": "npm run bootstrap-dist && tsc --build ./tsconfig.node.json",
        "postbuild-node": "npm run copy-package",
        "build-browser": "npm run bootstrap-dist && NODE_ENV=development webpack --mode=development --progress",
        "build-browser-production": "npm run bootstrap-dist && NODE_ENV=production webpack --mode=production --progress",
        "watch-browser": "npm run bootstrap-dist && webpack --progress --watch",
        "watch-node": "npm run bootstrap-dist && tsc --build ./tsconfig.node.json --watch ",
        "benchmarks": "npm run benchmark-publish && npm run benchmark-subscribe",
        "benchmark-publish": "node test/benchmarks/publish.js",
        "benchmark-subscribe": "node test/benchmarks/subscribe.js",
        "prebuild-benchmark": "npm run build -- --config-name=node-lib",
        "build-benchmark": "npm run benchmarks",
        "benchmark-build": "npm run build-benchmark",
        "benchmark": "npm run benchmarks",
        "watch": "webpack --progress --watch --mode=development",
        "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache/ '*/**/*.{js,ts}'",
        "test": "jest --detectOpenHandles",
        "test-unit": "jest  --verbose --useStderr --forceExit test/unit",
        "test-types": "npm run test-browser-types",
        "test-browser-types": "tsc -b tsconfig.browser.json",
        "coverage": "jest --coverage",
        "test-integration": "jest --verbose --useStderr --forceExit --testTimeout=40000 --runInBand test/integration/*.test.*",
        "test-end-to-end": "jest --verbose --useStderr --forceExit --testTimeout=40000 --runInBand test/end-to-end/*.test.*",
        "test-exports": "cd test/exports && npm run link && tsc --noEmit --project ./tsconfig.json && npm test",
        "presetup-browser": "npm run teardown-browser",
        "setup-browser": "node ./test/browser/server.js",
        "teardown-browser": "node -e 'http.request(\"http://localhost:8880/stop\").on(\"error\", () => {}).end()' || true",
        "test-browser-realtime": "npm run build-browser && concurrently -k --success=first 'node ./test/browser/server.js' 'sleep 5 && nightwatch --headless --verbose --timeout=30000 --config=nightwatch.conf.js ./test/browser/realtime.js'",
        "test-browser-resend": "npm run build-browser && concurrently -k --success=first 'node ./test/browser/server.js' 'sleep 5 && nightwatch --headless --verbose --timeout=30000 --config=nightwatch.conf.js ./test/browser/resend.js'",
        "clean": "jest --clearCache || true; rm -rf dist vendor *.tsbuildinfo node_modules/.cache || true",
        "docs": "typedoc --options typedoc.js"
    },
    "engines": {
        "node": ">= 12"
    },
    "author": "Streamr",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/plugin-proposal-decorators": "^7.16.4",
        "@babel/plugin-transform-modules-commonjs": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.4",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-typescript": "^7.16.0",
        "@ethersproject/wordlists": "^5.5.0",
        "@types/express": "^4.17.13",
        "@types/lodash": "^4.14.175",
        "@types/node": "^16.10.6",
        "@types/node-fetch": "^2.5.12",
        "@types/split2": "^3.2.1",
        "@types/uuid": "^8.3.1",
        "babel-loader": "^8.2.3",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "benchmark": "^2.1.4",
        "buffer": "^6.0.3",
        "bytes": "^3.1.1",
        "chromedriver": "^103.0.0",
        "concurrently": "^6.4.0",
        "cors": "^2.8.5",
        "crypto-browserify": "^3.12.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.25.3",
        "ethers": "^5.4.7",
        "express": "^4.17.1",
        "git-revision-webpack-plugin": "^5.0.0",
        "jest-leak-detector": "^27.3.1",
        "lodash-webpack-plugin": "^0.11.6",
        "nightwatch": "^1.7.12",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "pretty-bytes": "^5.6.0",
        "process": "^0.11.10",
        "streamr-test-utils": "^2.0.0",
        "terser-webpack-plugin": "^5.2.5",
        "ts-loader": "^9.2.6",
        "typedoc": "^0.22.5",
        "util": "^0.12.4",
        "weak-napi": "^2.0.2",
        "webpack": "^5.64.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-merge": "^5.8.0"
    },
    "#IMPORTANT": "babel-runtime must be in dependencies, not devDependencies",
    "dependencies": {
        "@babel/runtime": "^7.16.3",
        "@babel/runtime-corejs3": "^7.16.3",
        "@ethersproject/abi": "^5.5.0",
        "@ethersproject/abstract-signer": "^5.5.0",
        "@ethersproject/address": "^5.5.0",
        "@ethersproject/bignumber": "^5.5.0",
        "@ethersproject/bytes": "^5.5.0",
        "@ethersproject/constants": "^5.5.0",
        "@ethersproject/contracts": "^5.5.0",
        "@ethersproject/providers": "^5.5.0",
        "@ethersproject/transactions": "^5.5.0",
        "@ethersproject/wallet": "^5.5.0",
        "@ethersproject/web": "^5.5.0",
        "@types/debug": "^4.1.7",
        "ajv": "^8.8.2",
        "ajv-formats": "^2.1.1",
        "core-js": "^3.19.1",
        "debug": "^4.3.2",
        "env-paths": "^2.2.1",
        "eventemitter3": "^4.0.7",
        "idb-keyval": "^5.1.5",
        "lodash": "^4.17.21",
        "mem": "^8.1.1",
        "node-fetch": "^2.6.6",
        "p-limit": "^3.1.0",
        "p-memoize": "4.0.3",
        "quick-lru": "^6.0.1",
        "readable-stream": "^3.6.0",
        "reflect-metadata": "^0.1.13",
        "split2": "^3.2.2",
        "sqlite": "^4.0.23",
        "sqlite3": "^5.0.8",
        "streamr-client-protocol": "^12.0.0",
        "streamr-network": "^35.0.0",
        "ts-toolbelt": "^9.6.0",
        "tsyringe": "^4.6.0",
        "uuid": "^8.3.2"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.5",
        "utf-8-validate": "^5.0.7"
    },
    "bugs": {
        "url": "https://github.com/streamr-dev/streamr-client/issues"
    },
    "homepage": "https://github.com/streamr-dev/streamr-client#readme"
}
